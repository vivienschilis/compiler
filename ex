class Valeur is
Entier a; Valeur k
methods get(Entier c) returns Entier is { Valeur b begin b.a end }

class Valeur2 extends Valeur is
  Valeur2 val2; Entier k
methods set(Entier c) returns Valeur is { Valeur res begin res:= if (c>0) then val2 else val2 end } 
{
	Valeur v;
	Valeur f
	begin
	v := f 
	end	
}
